<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Panel - <%= title %></title>
    <link href="/static/styles/panel.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500|Roboto+Condensed|Roboto+Mono" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src="/static/scripts/jquery/3.1.1.js"></script>
</head>
<body class="panel">

    <div class="container">
        <div class="sidebar">
            <ul>
                <li class="brand">Sherlock</li>
                <%- include("partials/sidebar", data) %>
            </ul>
        </div>

        <div class="content">
            <div class="nav">
                <span><%= title %></span>
                <span class="logout"><a href="/panel/logout">Logout</a></span>
            </div>
            <div class="main">
                <%- include(content, data) %>
            </div>
        </div>
    </div>

</body>
</html>
